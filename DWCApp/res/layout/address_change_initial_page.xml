<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="Current Mobile"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etCurrentMobile"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="21dp"
            android:gravity="center"
            android:layout_marginStart="21dp"
            android:textColor="@color/white"
            android:layout_marginTop="10dp"
            android:background="@drawable/edit_text_background_disabled"
            android:inputType="phone" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="Current Fax"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etCurrentFax"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/white"
            android:layout_marginRight="21dp"
            android:gravity="center"
            android:layout_marginStart="21dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/edit_text_background_disabled" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="Current Email"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etCurrentEmail"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginRight="21dp"
            android:textColor="@color/white"
            android:gravity="center"
            android:layout_marginStart="21dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/edit_text_background_disabled" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="Current PO Box"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etCurrentPOBox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginRight="21dp"
            android:layout_marginStart="21dp"
            android:textColor="@color/white"
            android:layout_marginTop="10dp"
            android:background="@drawable/edit_text_background_disabled" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="New Mobile"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etNewMobile"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginRight="21dp"
            android:gravity="center"
            android:layout_marginStart="21dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/search_edit_text_background"
            android:imeOptions="actionNext"
            android:inputType="phone"
            android:singleLine="true" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="New Fax"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etNewFax"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginRight="21dp"
            android:layout_marginStart="21dp"
            android:gravity="center"
            android:layout_marginTop="10dp"
            android:background="@drawable/search_edit_text_background"
            android:imeOptions="actionNext"
            android:singleLine="true" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="New Email"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etNewEmail"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginRight="21dp"
            android:layout_marginStart="21dp"
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:background="@drawable/search_edit_text_background"
            android:imeOptions="actionNext"
            android:inputType="textEmailAddress"
            android:singleLine="true" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="21dp"
            android:layout_marginTop="21dp"
            android:text="New PO Box"
            android:textSize="12sp" />

        <EditText
            android:id="@+id/etNewPOBox"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_marginRight="21dp"
            android:gravity="center"
            android:layout_marginStart="21dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/search_edit_text_background"
            android:imeOptions="actionDone"
            android:singleLine="true" />


    </LinearLayout>
</ScrollView>